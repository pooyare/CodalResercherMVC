<body>
<div>
<div>
<hr>
<h3>
صورت سود و زیان تلفیقی
</h3>
<hr>
</div>
<br>

    <div ng-app="app.codal" ng-cloak="" style="margin-top: 100px; margin: 30px 10px 30px 10px;" class="statement-form">
        <div class="statement-container" ng-controller="statementController as ctrl">
            <div class="sheet-container" data-ng-repeat="sheet in ctrl.datasource.sheets">
                <div class="table-container" data-ng-repeat="table in sheet.tables">
                    <div class="table-title" ng-if="table.title || ''">{{table.title}}</div>
                    <div class="table-description" ng-if="table.description || ''">{{table.description}}</div>
                    <table class="rayanDynamicStatement">
                        <thead>
                            <tr ng-repeat="row in table.header.cells track by $index">
                                <th ng-repeat="cell in row  track by $index" rowspan="{{cell.rowspan}}" colspan="{{cell.colspan}}" ng-show="cell.isVisible">
                                    <span>{{cell.value | persianNumber}}</span>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="record in table.body.cells  track by $index">
                                <td ng-repeat="cell in record  track by $index" rowspan="{{cell.rowspan}}" colspan="{{cell.colspan}}" ng-class="cell.cssClass" ng-show="cell.isVisible">
                                    <span style="color: #055274;" ng-class="{'is-negative': ctrl.isNegative(cell)}">{{cell.value | persianNumber: 2}}
                                    </span>
                                    <br>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>

</div>
